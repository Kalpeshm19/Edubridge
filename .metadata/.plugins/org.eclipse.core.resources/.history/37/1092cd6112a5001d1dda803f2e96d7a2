package string;

public class Truck extends GroundVehicle {
	
	public double capacity;

	public Truck(int cargo) {
		super();
		this.capacity = capacity;
		System.out.println("Number of cargos::"+capacity);
	}
	@Override
	public void Drive()
	{
		super.Drive();
		System.out.println("Truck is moving");
	}
	
	public int loadCargo(int n)
	{
		
		System.out.println("Cargo is loaded:: "+cargo);
		
		return n;
	}
	

}
