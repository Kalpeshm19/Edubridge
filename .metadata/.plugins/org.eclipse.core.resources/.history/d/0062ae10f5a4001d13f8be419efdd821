package string;

public class Truck extends GroundVehicle {
	
	int cargo;

	public Truck(int cargo) {
		super();
		this.cargo = cargo;
	}
	
	public void Drive()
	{
		super.Drive();
		System.out.println("This is drive of truck");
	}
	
	public void loadCargo()
	{
		cargo=50;
		System.out.println("Cargo is loaded:: "+cargo);
	}
	

}
